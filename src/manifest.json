{
  "manifest_version": 3,
  "name": "eBird alerts TOC",
  "version": "1.0",
  "description": "Add a table of contents to eBird alerts pages",
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "css": ["css/content.css"],
      "matches": ["https://ebird.org/*alert/summary*"]
    }
  ]
}
